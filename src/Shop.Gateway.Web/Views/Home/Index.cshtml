@{
    ViewData["Title"] = "Home Page";
}
@model List<Shop.Common.GoodsList>

<div class="text-center">
    <ul>
        @foreach (var item in Model)
        {
            <li>
                <h3>@item.Title</h3>
                <span>@item.Price</span>
            </li>
        }
    </ul>
</div>
